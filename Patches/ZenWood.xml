		<success>Always</success>
		<Operations>
			<li Class="ExpandedWoodworking.PatchOperationModCompat">
				<modName>Zen Garden</modName>
			</li>				
			<li Class="PatchOperationRemove">
				<xpath>/ThingDefs/ThingDef[defName = "ZEN_Hedge"]/costList</xpath>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/ThingDefs/ThingDef[defName = "ZEN_Hedge"]</xpath>
				<value>
					<stuffCategories>
						<li>RawWoody</li>
					</stuffCategories>
					<costStuffCount>3</costStuffCount>
				</value>
			</li>
		</Operations>

	<!-- Advanced Biomes Lumber Recipes -->
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="ExpandedWoodworking.PatchOperationModCompat">
				<modName>Advanced Biomes</modName>
			</li>
			<li Class="ExpandedWoodworking.PatchOperationModCompat">
				<modName>[RF] Realistic Planets [1.0]</modName>
				<success>Invert</success>
			</li>
			<li Class="ExpandedWoodworking.PatchOperationModCompat">
				<modName>VGP Xtra Trees and Flowers</modName>
				<success>Invert</success>
			</li>
			<li Class="PatchOperationInsert">
				<xpath>Defs/RecipeDef[defName = "MakeWoodLumberBirch"]</xpath>
				<order>Prepend</order>
				<value>
					<RecipeDef ParentName="MakeWoodLumberBase">
						<defName>MakeWoodLumberAcacia</defName>
						<label>make acacia lumber</label>
						<description>Process raw acacia logs into lumber for construction. (AdvBiomes)</description>
						<allowMixingIngredients>false</allowMixingIngredients>
						<ingredients>
							<li>
								<filter>
									<thingDefs>
										<li>WoodLog_Acacia</li>
									</thingDefs>
								</filter>
								<count>15</count>
							</li>
						</ingredients>
						<fixedIngredientFilter>
							<thingDefs>
								<li>WoodLog_Acacia</li>
							</thingDefs>
						</fixedIngredientFilter>
						<products>
							<LumberAcacia>30</LumberAcacia>
						</products>
					</RecipeDef>
				</value>
			</li>
		</operations>
	</Operation>
	

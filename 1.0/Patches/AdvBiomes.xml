	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="ExpandedWoodworking.PatchOperationModCompat">
				<modName>Advanced Biomes</modName>
			</li>
			<li Class="ExpandedWoodworking.PatchOperationModCompat">
				<modName>[RF] Realistic Planets [1.0]</modName>
				<success>Invert</success>
			</li>
			<li Class="ExpandedWoodworking.PatchOperationModCompat">
				<modName>VGP Xtra Trees and Flowers</modName>
				<success>Invert</success>
			</li>
			<!-- Acacia Wood (only if not already present by RP or VGP) -->
			<li Class="PatchOperationInsert">
				<xpath>Defs/ThingDef[defName = "WoodLog_Birch"]</xpath>
				<order>Prepend</order>
				<value>
					<ThingDef ParentName="WoodLogBase">
						<defName>WoodLog_Acacia</defName> <!-- SG: 1.19  W: 1185  H: 3680 -->
						<label>acacia logs</label>
						<description>Raw wood from an acacia tree. When processed it becomes useful for building structures, weapons, and even simple prosthetics. Acacia is a very durable hardwood.  (AdvBiomes)</description>
						<graphicData>
							<color>(81,56,49)</color>
						</graphicData>
						<statBases>
							<Mass>1.2</Mass>
							<MarketValue>2.3</MarketValue>
							<DeteriorationRate>0.5</DeteriorationRate>
						</statBases>
						<stuffProps>
							<stuffAdjective>acacia</stuffAdjective>
							<commonality>0.25</commonality>
							<color>(81,56,49)</color>
							<statFactors>
								<MaxHitPoints>0.95</MaxHitPoints>
								<Beauty>0.95</Beauty>
								<WorkToMake>1.7</WorkToMake>
								<WorkToBuild>1.7</WorkToBuild>
								<DoorOpenSpeed>0.6</DoorOpenSpeed>
							</statFactors>
						</stuffProps>
						<butcherProducts>
							<LumberAcacia>30</LumberAcacia>
						</butcherProducts>		
					</ThingDef>
				</value>
			</li>	
		</operations>
	</Operation>
	
